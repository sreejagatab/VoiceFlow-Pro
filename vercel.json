{
  "version": 2,
  "name": "voiceflow-pro",
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/landing-page.html",
      "dest": "/public/landing-page.html"
    },
    {
      "src": "/demo-video",
      "dest": "/public/VoiceFlow Pro - Business Automation Voice Agent - Google Chrome 2025-07-27 16-23-50.mp4"
    },
    {
      "src": "/ASSEMBLYAI-SUBMISSION.md",
      "dest": "/public/ASSEMBLYAI-SUBMISSION.md"
    },
    {
      "src": "/case-studies/(.*)",
      "dest": "/public/case-studies/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/.*\\.(mp4|webm|ogg|avi)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
